{
  "name": "downloader-remotebrowser",
  "version": "0.0.0",
  "engines": {
    "node": "18.x"
  },
  "private": true,
  "type": "module",
  "scripts": {
    "prestart": "node node_modules/puppeteer/install.js",
    "start": "NODE_ENV=production node --optimize_for_size --max_old_space_size=460 --gc_interval=100 app.js",
    "start_dev": "NODE_ENV=dev nodemon -r dotenv/config app.js",
    "start_dev_traceGc": "NODE_ENV=dev node --trace_gc dotenv/config app.js",
    "test": "NODE_ENV=dev node test.js"
  },
  "dependencies": {
    "@mongodb-js/zstd": "^1.1.0",
    "@sentry/node": "^7.51.2",
    "@sentry/tracing": "^7.51.2",
    "agenda": "^5.0.0",
    "axios": "^1.4.0",
    "axios-retry": "^3.4.0",
    "body-parser": "^1.20.2",
    "check-disk-space": "^3.3.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "fingerprint-generator": "^2.1.27",
    "fingerprint-injector": "^2.1.27",
    "form-data": "^4.0.0",
    "helmet": "^7.0.0",
    "mongodb": "^5.4.0",
    "node-os-utils": "^1.3.7",
    "pidusage": "^3.0.2",
    "puppeteer": "^20.1.2",
    "puppeteer-cluster": "^0.23.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-adblocker": "^2.13.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.12"
  }
}
